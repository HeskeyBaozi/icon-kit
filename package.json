  "license": "MIT",
  "scripts": {
    "g": "cross-env KIT_FAST_GENERATE=true npm run generate",
    "generate": "cross-env NODE_ENV=development dotenv_config_path=kit/scripts/.env node -r dotenv/config -r ts-node/register kit/scripts/generate.ts",
    "build": "npm run build:esm && npm run build:cjs",
    "clean:esm": "rimraf ./es",
    "clean:cjs": "rimraf ./lib",
    "build:esm": "npm run clean:esm && cross-env NODE_ENV=production tsc --outDir ./es --project ./build.esm.tsconfig.json",
    "build:cjs": "npm run clean:cjs && cross-env NODE_ENV=production tsc --outDir ./lib --project ./build.cjs.tsconfig.json",
    "test": "npm run test:unit",
    "test:unit": "father test",
    "test:type-check": "tsc --noEmit --project kit/tsconfig.json"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@rgrove/parse-xml": "^2.0.1",
    "@types/chalk": "^2.2.0",
    "@types/debug": "^4.1.4",
    "@types/fs-extra": "^8.0.0",
    "@types/globby": "^9.1.0",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "@types/prettier": "^1.18.0",
    "@types/sax": "^1.2.0",
    "@types/signale": "^1.2.1",
    "@types/svgo": "^1.2.1",
    "@types/tapable": "^1.0.4",
    "@types/yargs-parser": "^13.0.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "father": "^2.13.7",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "sax": "^1.2.4",
    "signale": "^1.4.0",
    "svgo": "^1.3.0",
    "tapable": "^1.1.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "yargs-parser": "^13.1.1"
  }
